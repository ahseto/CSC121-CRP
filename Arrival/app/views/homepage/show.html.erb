<div class="container-fluid">
<table class="table table-hover">
  <thead class="thead-light">
    <tr>
      <th scope="col">Carrier</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody>
    <% @carriers.each do |s| %>
      <tr>
      <td><%= s.name %></td>
      <td><%= s.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

